{
  "hosting": [
    {
      "rewrites": [ {
        "source": "/sitemap",
        "function": "siteMap"
      },{
        "source": "**",
        "destination": "/index.html"
      }
      ],
      "target": "thailand",
      "public": "th/public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "rewrites": [ {
        "source": "**",
        "destination": "/index.html"
      }],
      "target": "main",
      "public": "main/public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ]
}
